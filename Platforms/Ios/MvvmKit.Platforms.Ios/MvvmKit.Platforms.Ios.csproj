<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0-ios</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <AnalysisLevel>latest-Recommended</AnalysisLevel>
        <LangVersion>latest</LangVersion>
        <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
        <SupportedOSPlatformVersion>15.4</SupportedOSPlatformVersion>
    </PropertyGroup>

    <PropertyGroup>
        <PackageId>MvvmKit.Platforms.Ios</PackageId>
        <Version>0.0.1</Version>
        <Authors>Diego Stamigni</Authors>
        <Company>Area Engineering Ltd</Company>
        <PackageTags>mvvm;mvvmkit</PackageTags>
    </PropertyGroup>

    <ItemGroup>
      <ProjectReference Include="..\MvvmKit.Platforms.Ios.Abstractions\MvvmKit.Platforms.Ios.Abstractions.csproj" />
      <ProjectReference Include="..\..\..\MvvmKit\MvvmKit.csproj" />
    </ItemGroup>

</Project>
